{
  "name": "latte",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:server": "jest --config jest.config.js",
    "test:server:watch": "jest --config jest.config.js --watch",
    "test:server:coverage": "jest --config jest.config.js --coverage",
    "test:all": "npm run test:run && npm run test:server",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "server:test": "NODE_ENV=test node server/server.js",
    "start:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "dev:full": "npm run start:all"
  },
  "dependencies": {
    "animejs": "^4.1.2",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",

    "morgan": "^1.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.7.1",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.19",
    "validator": "^13.15.15"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  }
}
